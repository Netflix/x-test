{
  "name": "@netflix/x-test",
  "description": "a simple, tap-compliant test runner for the browser",
  "version": "2.0.0-rc.1",
  "license": "Apache-2.0",
  "repository": "https://github.com/Netflix/x-test",
  "type": "module",
  "exports": {
    "./x-test.js": "./x-test.js"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "scripts": {
    "start": "node ./server.js",
    "lint": "eslint --max-warnings=0 .",
    "lint-fix": "eslint --fix .",
    "test": "npm run test:puppeteer:chrome",
    "test:puppeteer:chrome": "x-test --client=puppeteer --url=http://127.0.0.1:8080/test/ --coverage=true",
    "bump": "./bump.sh"
  },
  "files": [
    "LICENSE",
    "/x-test-reporter.css.js",
    "/x-test-reporter.js",
    "/x-test-root.js",
    "/x-test-suite.js",
    "/x-test-tap.js",
    "/x-test.js"
  ],
  "dependencies": {},
  "devDependencies": {
    "@netflix/x-test-cli": "1.0.0-rc.2",
    "eslint": "^9.20.1"
  },
  "engines": {
    "node": ">=20.18",
    "npm": ">=10.8"
  },
  "contributors": [
    {
      "name": "Andrew Seier",
      "email": "aseier@netflix.com"
    },
    {
      "name": "Casey Klebba",
      "email": "cklebba@netflix.com"
    }
  ]
}
