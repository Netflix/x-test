{
  "name": "x-test",
  "version": "1.0.0-rc.1",
  "description": "a simple, tap-compliant test runner for the browser",
  "main": "x-test.js",
  "repository": "netflix/x-test",
  "author": "Andrew Seier",
  "license": "SEE LICENSE IN LICENSE",
  "config": {
    "port": 8080,
    "host": "127.0.0.1"
  },
  "scripts": {
    "start": "echo View at: http://$npm_package_config_host:$npm_package_config_port && static ./ --cache 1 --host-address $npm_package_config_host --port $npm_package_config_port > /dev/null 2>&1",
    "lint": "eslint --quiet .",
    "lint-strict": "eslint .",
    "test": "node test.js | tap-parser -l"
  },
  "files": [
    "LICENSE",
    "/index.html",
    "/index.js",
    "/test.js",
    "/x-test.js",
    "/test"
  ],
  "devDependencies": {
    "node-static": "0.7.11",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-plugin-prettier": "^3.0.1",
    "prettier": "^1.17.0",
    "faucet": "^0.0.1",
    "puppeteer": "^1.11.0",
    "tap-parser": "^9.0.0"
  }
}
